{
    "name": "hgabka/doctrine-translatable",
    "type": "library",
    "description": "Translatable behaviour extension for Doctrine2",
    "keywords": [
        "doctrine2",
        "extensions",
        "translatable",
        "i18n"
    ],
    "homepage": "http://hgabka.eu",
    "authors": [{
        "name": "Gabe"
    }],
    "require": {
        "php": ">=8.0.3",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.0",
        "doctrine/inflector": "^2.0",
        "doctrine/persistence": "^2.0",
        "jms/metadata": "^1.1|^2.0"
    },
    "require-dev": {
        "symfony/yaml": "6.0.*"
    },
    "autoload": {
        "psr-0": { "Hgabka\\Doctrine\\Translatable": "lib/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
